import static java.lang.System.*;

public class Name
{
	private String name;

	public Name()
	{
		setName("");

	}

	public Name(String s)
	{
		setName(s);

	}

   public void setName(String s)
   {
	   name = s;

   }

	public String getFirst()
	{
		
		return "";
	}

	public String getLast()
	{
		s.substring(0,s.indexOf(" "))
		return "";
	}

 	public String toString()
 	{
 		return "";
	}
}