import static java.lang.System.*;
import java.util.Scanner;
public class Word2 implements Comparable<Word>
{
	private String word;

	public Word2( String s)
	{
		word = s; 

	}

	private int numVowels()
	{
		String vowels = "AEIOUaeiou";
		int vowelCount=0;
		Scanner chop = new Scanner(word);
		while (chop.hasNext()){
			if (vowels.indexOf(chop.next())!=-1){
				vowelCount++;
			}
		}






		return vowelCount;
	}

	public int compareTo(Word2 rhs)
	{
		if(this.numVowels() > rhs.numVowels())
		{
			return 1;
		}
		else if (this.numVowels() < rhs.numVowels())
		{
			return -1;
		}
		else
		{
			return this.toString().compareTo(rhs.toString());
			
		}


	
	}

	public String toString()
	{
		return word;
	}
}