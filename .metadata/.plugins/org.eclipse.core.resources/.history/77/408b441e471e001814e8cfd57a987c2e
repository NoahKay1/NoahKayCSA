
public class LabAssessment {
	public static void main(String args[])
	{
		LabAssessment test = new LabAssessment();
		test.isGoofy(20);
	}

	public static boolean isGoofy(int num){
		int number = num; 
		int next = 0;
		int orig = num;
		int sum = 0;
		int right = 0;
		String count = "" + num;
		int[] arr = new int[count.length()];
		while (number >= 0){
			next = number % 10;
			number = number / 10;
			arr[right] = next;
			right++;
		}
		for (int i = 0; i< count.length(); i++){
			if (orig % arr[i] == 0){
				sum+=arr[i];
			}
		}
		if (sum % 2 == 1){
			return true;
		}
		return false;
		
	}

}